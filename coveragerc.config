[run]
source = src/etl_studio
omit = 
    */tests/*
    */test/*
    */__pycache__/*
    */site-packages/*
    */.venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = test-reports/coverage
